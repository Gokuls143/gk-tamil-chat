{
  "builds": [
    {
      "src": "build/libs/demo-0.0.1-SNAPSHOT.jar",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "src/main/resources/static"
      }
    }
  ],
  "env": {
    "JAVA_VERSION": "21"
  },
  "routes": [
    {"src": "/(.*)", "dest": "/src/main/resources/static/$1"}
  ]
}
